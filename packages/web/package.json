{
  "name": "@homzhub/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f .env react-app-rewired start",
    "build": "GENERATE_SOURCEMAP=false react-app-rewired build",
    "build:dev": "env-cmd -f .env.development yarn run build",
    "build:staging": "env-cmd -f .env.staging yarn run build",
    "build:production": "env-cmd -f .env.production yarn run build",
    "test": "CI=true react-app-rewired test --coverage --env=jsdom",
    "eject": "react-app-rewired eject",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "format": "prettier --write './**/*.{ts,tsx,scss,css,js,json}'",
    "sonar": "ts-node sonar-project.ts"
  },
  "dependencies": {
    "@homzhub/common": "0.0.1",
    "@react-google-maps/api": "^2.1.0",
    "@types/react-helmet": "^6.1.0",
    "apexcharts": "^3.22.2",
    "crypto-js": "^4.0.0",
    "env-cmd": "^10.1.0",
    "exenv": "^1.2.2",
    "node-sass": "^8.0.0",
    "react-apexcharts": "^1.3.7",
    "react-burger-menu": "3.0.2",
    "react-circular-progressbar": "^2.0.3",
    "react-datepicker": "^3.3.0",
    "react-dom": "17.0.2",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.2.2",
    "react-helmet": "^6.1.0",
    "react-html-parser": "2.0.2",
    "react-infinite-scroller": "^1.2.4",
    "react-load-script": "^0.0.6",
    "react-markdown": "^5.0.3",
    "react-multi-carousel": "^2.5.5",
    "react-native-web": "^0.12.3",
    "react-router-breadcrumbs-hoc": "^3.3.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "^3.4.1",
    "react-tooltip": "^4.2.10",
    "react-webcam": "^5.2.3",
    "reactjs-popup": "^2.0.4",
    "react-calendar": "^3.3.1",
    "mixpanel-browser": "^2.41.0",
    "array-move": "3.0.1",
    "react-sortable-hoc": "2.0.0",
    "react-facebook-pixel": "1.0.4",
    "query-string": "7.0.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^3.1.47",
    "@types/exenv": "^1.2.0",
    "@types/googlemaps": "^3.43.1",
    "@types/react-burger-menu": "2.2.0",
    "@types/react-calendar": "^3.1.4",
    "@types/react-datepicker": "^3.1.2",
    "@types/react-dom": "^16.9.8",
    "@types/react-facebook-login": "^4.1.3",
    "@types/react-html-parser": "^2.0.1",
    "@types/react-infinite-scroller": "^1.2.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-slick": "^0.23.4",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-react-native-web": "^0.12.3",
    "customize-cra": "^1.0.0",
    "react-app-rewired": "^2.1.6",
    "@types/mixpanel-browser": "^2.35.6"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "!src/index.tsx"
    ],
    "coverageReporters": [
      "text"
    ]
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  }
}
