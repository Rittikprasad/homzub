{
  "name": "@homzhub/mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "cross-env ENVFILE=.env.development && react-native run-android",
    "android:partner": "SET ENVFILE=.env.development && react-native run-android --variant=partnerDebug",
    "android:clean": "cd android && ./gradlew clean",
    "android:bundle": "cross-env ENVFILE=.env.development react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android:build": "cross-env env:ENVFILE=.env yarn android:clean && yarn android:bundle && cd android && ./gradlew assembleDebug ",
    "android:bundle:prod": "ENVFILE=.env react-native bundle --platform android --dev false --entry-file packages/mobile/index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android:build:prod": "ENVFILE=.env yarn android:clean && yarn android:bundle:prod && cd android && ./gradlew assembleHomzhubRelease -x bundleHomzhubReleaseJsAndAssets",
    "android:build:aab": "ENVFILE=.env yarn android:clean && yarn android:bundle:prod && cd android && ./gradlew bundleHomzhubRelease -x bundleHomzhubReleaseJsAndAssets",
    "android:build:partner:prod": "ENVFILE=.env yarn android:clean && yarn android:bundle:prod && cd android && ./gradlew assemblePartnerRelease -x bundlePartnerReleaseJsAndAssets",
    "ios": "ENVFILE=.env.development react-native run-ios --configuration Debug --scheme homzhub",
    "ios:partner": "ENVFILE=.env react-native run-ios --configuration Debug --scheme homzhub-partner",
    "ios:clean": "rm -rf ios/build",
    "pod:install": "cd ios && pod install",
    "ios:build": "ENVFILE=.env.development yarn ios:clean && yarn ios:bundle",
    "ios:bundle": "ENVFILE=.env.development react-native bundle --platform ios --dev false --entry-file packages/mobile/index.js --bundle-output ios/main.jsbundle --assets-dest ios",
    "ios:bundle:prod": "ENVFILE=.env react-native bundle --platform ios --dev false --entry-file packages/mobile/index.js --bundle-output ios/main.jsbundle --assets-dest ios",
    "ios:build:prod": "ENVFILE=.env yarn ios:clean && yarn ios:bundle:prod",
    "start": "react-native start",
    "test": "ENVFILE=.env.development jest --coverage --env=jsdom",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "format": "prettier --write './**/*.{ts,tsx,js,json}'",
    "sonar": "ts-node sonar-project.ts",
    "postinstall": "jetify"
  },
  "dependencies": {
    "@babel/plugin-transform-export-namespace-from": "^7.24.7",
