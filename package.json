{
  "name": "homzhub",
  "private": true,
  "version": "1.0.0",
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": []
  },
  "scripts": {
    "lint": "yarn workspaces run lint",
    "lint:fix": "yarn workspaces run lint:fix",
    "lint:common": "yarn workspace @homzhub/common run lint",
    "lint:mobile": "yarn workspace @homzhub/mobile run lint",
    "lint:web": "yarn workspace @homzhub/web run lint",
    "start:web": "yarn workspace @homzhub/web run start",
    "start:mobile": "yarn workspace @homzhub/mobile run start",
    "start:ffm": "yarn workspace @homzhub/ffm run start",
    "test": "yarn workspaces run test",
    "test:common": "yarn workspace @homzhub/common run test",
    "test:mobile": "yarn workspace @homzhub/mobile run test",
    "test:web": "yarn workspace @homzhub/web run test",
    "build:web": "yarn workspace @homzhub/web run build:dev",
    "build:web:stag": "yarn workspace @homzhub/web run build:staging",
    "build:web:prod": "yarn workspace @homzhub/web run build:production",
    "android": "jetify && yarn workspace @homzhub/mobile run android",
    "android:partner": "jetify && yarn workspace @homzhub/mobile run android:partner",
    "android:build": "jetify && yarn workspace @homzhub/mobile run android:build",
    "android:build:prod": "jetify && yarn workspace @homzhub/mobile run android:build:prod",
    "android:build:prod:aab": "jetify && yarn workspace @homzhub/mobile run android:build:prod:aab",
    "android:build:aab": "jetify && yarn workspace @homzhub/mobile run android:build:aab",
    "android:build:partner:prod": "jetify && yarn workspace @homzhub/mobile run android:build:partner:prod",
    "android:build:ffm": "jetify && yarn workspace @homzhub/ffm run android:build",
    "ios": "yarn workspace @homzhub/mobile run ios",
    "ios:partner": "yarn workspace @homzhub/mobile run ios:partner",
    "pod:install": "yarn workspace @homzhub/mobile run pod:install",
    "ios:build": "yarn workspace @homzhub/mobile run ios:build",
    "ios:build:prod": "yarn workspace @homzhub/mobile run ios:build:prod",
    "ios:build:partner:prod": "yarn workspace @homzhub/mobile run ios:build:partner:prod",
    "android:ffm": "jetify && yarn workspace @homzhub/ffm run android",
    "android:build:prod:ffm": "jetify && yarn workspace @homzhub/ffm run android:build:prod",
    "android:build:aab:ffm": "jetify && yarn workspace @homzhub/ffm run android:build:aab",
    "ios:build:ffm": "yarn workspace @homzhub/ffm run ios:build",
    "ios:ffm": "yarn workspace @homzhub/ffm run ios",
    "pod:install:ffm": "yarn workspace @homzhub/ffm run pod:install",
    "format": "yarn workspaces run format",
    "sonar": "yarn workspaces run sonar",
    "premerge": "yarn format && yarn lint && yarn test -u --passWithNoTests",
    "postinstall": "jetify"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "deprecated-react-native-prop-types": "3.0.2",
    "formik": "^2.1.5",
    "get-yarn-workspaces": "^1.0.2",
    "i18next": "^19.4.4",
    "jetifier": "^2.0.0",
    "json2typescript": "^1.4.1",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "patch-package": "^8.0.0",
    "react": "18.1.0",
    "react-dropzone": "^11.3.1",
    "react-i18next": "^11.4.0",
    "react-native": "0.73.5",
    "react-native-device-info": "^8.0.1",
    "react-native-flash-message": "^0.1.15",
    "react-native-progress": "^4.1.2",
    "react-native-youtube": "^2.0.1",
    "react-redux": "^7.2.0",
    "redux": "^5.0.1",
    "redux-devtools-extension": "2.13.9",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "url-regex": "^5.0.0",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "0.73.2",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/i18next": "^13.0.0",
    "@types/jest": "^29.2.1",
    "@types/jsdom": "^12.2.4",
    "@types/lodash": "^4.14.154",
    "@types/react": "^18.2.6",
    "@types/react-i18next": "^8.1.0",
    "@types/react-native": "^0.62.7",
    "@types/react-redux": "^7.1.8",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-devtools-extension": "2.13.2",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-mock-store": "^1.0.2",
    "@types/yup": "^0.28.3",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.8.1",
    "jest": "^29.2.1",
    "jest-enzyme": "^7.0.2",
    "jest-sonar-reporter": "^2.0.0",
    "jsdom": "^15.1.1",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "redux-mock-store": "^1.5.4",
    "sonarqube-scanner": "^2.6.0",
    "ts-node": "^8.10.1",
    "typescript": "5.0.4"
  },
  "resolutions": {
    "react-native": "0.73.5",
    "react-native-maps": "^0.27.1",
    "react-native-screens": "^3.29.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,css,less,ejs,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
